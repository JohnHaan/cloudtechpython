#!/projects/cloudtechpython/20170605/ENV_dolbam/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'dolbam==0.0.2','console_scripts','dolbam'
__requires__ = 'dolbam==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('dolbam==0.0.2', 'console_scripts', 'dolbam')()
    )
