#!/projects/cloudtechpython/20170605/ENV_sdhong/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'sdhong==0.0.2','console_scripts','sdhong'
__requires__ = 'sdhong==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('sdhong==0.0.2', 'console_scripts', 'sdhong')()
    )
