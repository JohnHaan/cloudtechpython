#!/projects/cloudtechpython/20170605/ENV_zeroxkim/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'zeroxkim==0.0.2','console_scripts','zeroxkim'
__requires__ = 'zeroxkim==0.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('zeroxkim==0.0.2', 'console_scripts', 'zeroxkim')()
    )
