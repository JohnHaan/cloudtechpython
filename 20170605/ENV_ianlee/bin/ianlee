#!/projects/cloudtechpython/20170605/ENV_ianlee/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'ianlee==0.0.4','console_scripts','ianlee'
__requires__ = 'ianlee==0.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ianlee==0.0.4', 'console_scripts', 'ianlee')()
    )
